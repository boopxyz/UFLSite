        <img class="absolute top-0 left-0 w-full h-24 object-cover" src="https://images.unsplash.com/photo-1592580119984-d31e3833fa78?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&amp;ixlib=rb-1.2.1&amp;auto=format&amp;fit=crop&amp;w=800&amp;q=80">
        <img class="relative w-16 h-16 mb-4 mx-auto rounded-full object-cover object-right" src="https://i.imgur.com/Uok2Coa.png" alt="">
        <div class="px-6 text-center">
          <h3 class="font-medium"><%= pUser.userUsername %></h3>
          <p class="text-sm text-gray-500"><%= pUser.userInformation[0].userTitle %></p>
          <p class="mb-6 text-sm text-gray-500 opacity-50"><%= user.userID %></p>
          <% if (typeof pUser.linkedAccounts[0].roblox != "undefined") { %>
            <div class="flex items-center justify-center mb-6">
              <span class="mr-2">
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="18" height="14" viewBox="0 0 24 24"><path d="M13.383 14.341l-3.726-.958l.959-3.726l3.726.959l-.96 3.726zM4.913 0L0 19.088L19.088 24L24 4.912L4.912 0z" fill="currentColor"></path></svg>
              </span>
              <p class="text-sm"><%= pUser.linkedAccounts[0].roblox.robloxUsername %></p>
            </div>
          <% } %>
          <% if (typeof pUser.linkedAccounts[0].discord != "undefined") { %>
            <div class="flex items-center justify-center mb-6">
            <span class="mr-2">
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="18" height="14" viewBox="0 0 24 24"><path d="M20.317 4.37a19.791 19.791 0 0 0-4.885-1.515a.074.074 0 0 0-.079.037c-.21.375-.444.864-.608 1.25a18.27 18.27 0 0 0-5.487 0a12.64 12.64 0 0 0-.617-1.25a.077.077 0 0 0-.079-.037A19.736 19.736 0 0 0 3.677 4.37a.07.07 0 0 0-.032.027C.533 9.046-.32 13.58.099 18.057a.082.082 0 0 0 .031.057a19.9 19.9 0 0 0 5.993 3.03a.078.078 0 0 0 .084-.028c.462-.63.874-1.295 1.226-1.994a.076.076 0 0 0-.041-.106a13.107 13.107 0 0 1-1.872-.892a.077.077 0 0 1-.008-.128a10.2 10.2 0 0 0 .372-.292a.074.074 0 0 1 .077-.01c3.928 1.793 8.18 1.793 12.062 0a.074.074 0 0 1 .078.01c.12.098.246.198.373.292a.077.077 0 0 1-.006.127a12.299 12.299 0 0 1-1.873.892a.077.077 0 0 0-.041.107c.36.698.772 1.362 1.225 1.993a.076.076 0 0 0 .084.028a19.839 19.839 0 0 0 6.002-3.03a.077.077 0 0 0 .032-.054c.5-5.177-.838-9.674-3.549-13.66a.061.061 0 0 0-.031-.03zM8.02 15.33c-1.183 0-2.157-1.085-2.157-2.419c0-1.333.956-2.419 2.157-2.419c1.21 0 2.176 1.096 2.157 2.42c0 1.333-.956 2.418-2.157 2.418zm7.975 0c-1.183 0-2.157-1.085-2.157-2.419c0-1.333.955-2.419 2.157-2.419c1.21 0 2.176 1.096 2.157 2.42c0 1.333-.946 2.418-2.157 2.418z" fill="currentColor"></path></svg>
            </span>
            <p class="text-sm"><%= pUser.linkedAccounts[0].discord.discordUsername %></p>
          </div>
          <% } %>
        </div>
      </div>
    </div>
  </section>

  <%- include("partials/footer") %>