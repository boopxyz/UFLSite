<%- include("partials/dashnavbar", { user: user }) %>

<section class="py-8 bg-gray-100">
    <div class="container px-4 mx-auto">
      <div class="group relative pt-14 bg-white shadow rounded overflow-hidden">
        <button class="transform absolute top-0 right-0 mr-6 mt-7 z-10 motion-safe:hover:scale-150">
          <svg class="h-3 w-3 text-white" viewBox="0 0 12 4" fill="none" xmlns="http://www.w3.org/2000/svg">
