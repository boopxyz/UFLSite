<% } else { %>
  <div class="bg-gray-800">
  <div class="py-8 px-6">
    <div class="container px-4 mx-auto">
      <h2 class="text-2xl font-bold text-white" data-config-id="10_header">Welcome, <%= user.userUsername %> &#x1F44B;</h2>
    </div>
  </div>
  
<section class="py-8">
  <div class="container px-4 mx-auto">
    <div class="pt-16 px-16 text-center bg-purple-500 rounded">
